#Types:
print(type(5))
my_dict = {}
print(type(my_dict))
my_list = []
print(type(my_list))

#Class:
class Facade:
  pass
facade_1 = Facade() #Instantiation
facade_1_type = type(facade_1)
print(facade_1_type)

#Variable Printing:
class Grade:
  minimum_passing = 65

grade = Grade()
print(grade.minimum_passing)
